{
  "name": "fume-cli",
  "description": "fume command line interface",
  "version": "0.1.18",
  "author": "acidjazz @acidjazz",
  "bin": {
    "fume": "bin/run"
  },
  "bugs": "https://github.com/fumeapp/fume-cli/issues",
  "dependencies": {
    "@auth0/s3": "^1.0.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/fs-extra": "^9.0.1",
    "@types/js-yaml": "^4.0.1",
    "@types/numeral": "^2.0.1",
    "adm-zip": "^0.5.5",
    "aws-sdk": "^2.756.0",
    "axios": "^0.21.0",
    "cli-ux": "^5.5.0",
    "death": "^1.1.0",
    "enquirer": "^2.3.6",
    "envfile": "^6.14.0",
    "execa": "^5.0.0",
    "fd-slicer": "^1.1.0",
    "findit2": "^2.2.3",
    "fs-extra": "^10.0.0",
    "get-folder-size": "^2.0.1",
    "inquirer": "^8.1.1",
    "js-yaml": "^4.1.0",
    "listr": "^0.14.3",
    "listr2": "^3.1.1",
    "md5-file": "^5.0.0",
    "mime": "^2.4.6",
    "mkdirp": "^1.0.4",
    "numeral": "^2.0.6",
    "pend": "^1.2.0",
    "raven": "^2.6.4",
    "rimraf": "^3.0.2",
    "streamsink": "^1.2.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/adm-zip": "^0.4.34",
    "@types/chai": "^4",
    "@types/death": "^1.1.1",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^8",
    "@types/node": "^15.12.2",
    "chai": "^4",
    "eslint": "^7.9",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11",
    "mocha": "^9",
    "nyc": "^15",
    "ts-node": "^10",
    "typescript": "^4.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/src/assets",
    "/src/lib/s3",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/fumeapp/fume-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fume",
    "env": "production",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "auth": {
        "description": "manage fume authentication"
      }
    },
    "hooks": {
      "init": "./lib/hooks/init/init"
    }
  },
  "repository": "fumeapp/fume-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
